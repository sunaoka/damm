{
    "name": "sunaoka/damm",
    "description": "A PHP library that calculates and verifies the check digit using the Damm algorithm.",
    "keywords": ["damm", "check digit"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^8.5 || ^9.6 || ^10.5 || ^11.5 || ^12.3"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\Damm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
